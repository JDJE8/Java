public class Factorial {
   float fa=1,f;
   void getdata(float a){
       f=a;
   }
   float Factorial_res(){
       for(int i=1;i<=f;i++){
           fa=fa*i;
       }
       return fa;
   }
   public static void main(String args[]){
       Factorial f = new Factorial();
       f.getdata(7);
       float fact = f.Factorial_res();
       System.out.println("Factorial of 5 is "+fact);
   }
}
