import javax.swing.*;
class Frame_Eg2 extends JFrame{
    Frame_Eg2(){
        JButton B1 = new JButton(" Click Me ");
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        add(B1);
        setSize(500,500);
        //setLayout(null);
        setVisible(true);
    }
    public static void main(String args[]){
        new Frame_Eg2();
    }
}
