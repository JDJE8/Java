import java.util.*;
class Overloading{
    float b,h,r;
    void Area_rectangle(){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter length and breadth ");
        int l=sc.nextInt();
        int b=sc.nextInt();
        int rec=l*b;
        System.out.println("Area of Rectangle is "+rec);
    }
    void Area_triangle(int b,int h){
        float a=(float)(0.5)*b*h;
        System.out.println("Area of triangle is "+a);
    }
    void Area_circle(int rad){
        r=rad;
        float circle=(float)3.14*r*r;
        
    }
}
